<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.web>
    <!-- Enables Tracing -->
    <trace enabled="true" pageOutput="true" />

    <!-- Enables Debugging -->
    <compilation debug="true" />
      <pages validateRequest="false" />

  </system.web>

 <system.webServer>
    <!-- Directory Listing -->
    <directoryBrowse enabled="true" />
  </system.webServer>

</configuration>
